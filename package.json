    "@radix-ui/react-tabs": "^1.1.13",
    "@react-spring/web": "^9.7.5",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.4.2",
    "@vercel/blob": "^2.0.0",
    "bcryptjs": "^3.0.3",

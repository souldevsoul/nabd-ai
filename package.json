{
  "name": "orbita",
  "version": "0.1.0",
  "description": "ORBITA - Kazakhstan AI Marketplace - Connect with AI Experts",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:product": "eslint . --config eslint.config.product.mjs",
    "generate:assets": "tsx scripts/generate-assets.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "test:e2e": "PLAYWRIGHT_BROWSERS_PATH=~/.cache/playwright playwright test",
    "test:e2e:ui": "PLAYWRIGHT_BROWSERS_PATH=~/.cache/playwright playwright test --ui",
    "test:e2e:headed": "PLAYWRIGHT_BROWSERS_PATH=~/.cache/playwright playwright test --headed",
    "test:e2e:debug": "PLAYWRIGHT_BROWSERS_PATH=~/.cache/playwright playwright test --debug",
    "test:e2e:report": "PLAYWRIGHT_BROWSERS_PATH=~/.cache/playwright playwright show-report",
    "postinstall": "prisma generate"
  },
